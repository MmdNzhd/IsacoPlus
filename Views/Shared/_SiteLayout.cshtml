        var returnUrl = "/";
        function addToShopBasket(code) {
            var products = [];
            var oldCookieValue = $.cookie('cart');
            var pId = code;
            products[0] = pId;
            if (oldCookieValue == undefined) {
                $.cookie('cart', products, { expires: 7, path: '/' });

            } else {

                var value = oldCookieValue + "," + new String(products);
                value = removeDuolicate(value);
                $.removeCookie('cart', { path: '/' });
                $.cookie('cart', value, { expires: 7, path: '/' });
            }
            products = [];
            var cooki = $.cookie('cart');
            var productCount = cooki.split(",").length;
            $('#cartCount').text(productCount);
            Toast.fire({
                icon: 'success',
                title: 'به سبد خرید شما اضافه شد'

            })
        }
        function removeDuolicate(a) {
            var cookieVal = a.split(",");
            var realValue = [];
            for (i = 0; i < cookieVal.length; i++) {
                if (jQuery.inArray(cookieVal[i], realValue) !== -1) {
                    continue;
                }
                else {
                    realValue.push(cookieVal[i]);
                }
            }
            return realValue;
        }

        document.getElementById('searchItem').onkeypress = function (e) {
            if (!e) e = window.event;
            var keyCode = e.keyCode || e.which;
            if (keyCode == '13') {
                location.href = "/Search/" + $("#searchItem").val();
            }
        }
        function Search() {
            location.href = "/Search/" + $("#searchItem").val();
        }
        var oldCookieValue = $.cookie('cart');
        if (oldCookieValue == "") {
            $.removeCookie('cart', { path: '/' });
        }
        if (oldCookieValue != undefined) {
            if (oldCookieValue && oldCookieValue.includes(",,")) {
                oldCookieValue = oldCookieValue.replace(",,", ",");
                $.cookie('cart', oldCookieValue, { expires: 7, path: '/' });
            }
            var productCount = oldCookieValue.split(",").length;
            $('#cartCount').text(productCount);
        }
        $("#searchBox").keypress(function (event) {
            if (event.keyCode === 13) {
                window.location.href = "/Search/" + $("#searchBox").val();
            }
        });



        var $counter = $("#cartCount");
        var cartVal = $counter.html().parseInt;
        cartVal <= 0 ? $counter.hide() : $counter.css("display","inline-block");
    



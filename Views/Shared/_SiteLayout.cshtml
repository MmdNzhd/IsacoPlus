                            icon: 'success',
                            title: data.message

                        })
                        $("#login-form").addClass("d-none");
                        $("#Register-form").addClass("d-none");
                        $("#label-1").addClass("d-none");
                        $("#label-2").addClass("d-none");
                        $("#VerifyForm").removeClass("d-none");



                    }


                    if (data.status == 0) {
                        Toast.fire({
                            icon: 'error',
                            title: data.message
                        })
                    }


                }
            });

        })
    </script>
</body>

</html>



